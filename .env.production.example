# Groq API Configuration
# Get your API key from: https://console.groq.com/keys
REACT_APP_GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE

# Proxy Configuration
REACT_APP_USE_PROXY=true
REACT_APP_PROXY_URL=https://your-domain.com/api/chat.php

# Analytics Configuration
REACT_APP_STATS_PASSWORD=your_secure_password_here
REACT_APP_ANALYTICS_URL=https://your-domain.com/api/analytics.php

# Location Tracking (optional)
REACT_APP_ENABLE_LOCATION=true
REACT_APP_LOCATION_URL=https://your-domain.com/api/location.php
